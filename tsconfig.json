{
 "compilerOptions": {
    "allowJs": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    
	"target": "es5",
	"outDir": "./",
	
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
 },
 "include": ["../**/*",],
 "exclude": ["node_modules","dist","../type/*",]
}